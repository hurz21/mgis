<Window x:Class="winAlleLayer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:webgiscontrol"
        mc:Ignorable="d"
        Title="winAlleLayer" Height="300" Width="300" WindowStartupLocation="CenterScreen">
    <DockPanel>
        <StackPanel>
            <GroupBox Header="Maßstab" Margin="10">   
            <StackPanel Orientation="Horizontal" >
                <RadioButton Name="rbGross" GroupName="range" Content=" gross " IsChecked="True"/>
                <RadioButton Name="rbmittel" GroupName="range" Content=" mittel "/>
                <RadioButton Name="rbklein" GroupName="range" Content=" klein "/>
            </StackPanel>
            </GroupBox>
            <Button Name="Start" Content="start" Click="Start_Click"  Width="80"/>
            <TextBlock Name="tbinfo" Text="saoidasoid"></TextBlock>
            <Button Name="btnDateiaufrufen" Click="btnDateiaufrufen_Click" Content="Logfile"  Width="80"/>
            <TextBlock Name="tberrorcnt" Text="bitte warten ..."></TextBlock>
            <Button Name="cbmserrorExt" Content="ms_error.txt" Click="cbmserrorExt_Click"    Width="80" />
        </StackPanel>  
    </DockPanel>
</Window>
